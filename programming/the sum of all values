#include<iostream>
using namespace std;
int main()
{
   float a,b,c,d;
   int num;
   cout<<"enter value of a=";
   cin>>a;
   cout<<"enter value of b=";
   cin>>b;
   cout<<"enter value of c=";
   cin>>c;
   cout<<"enter value of d=";
   cin>>d;
   cout<<"enter a num 2 value=";
   cin>>num;
   cout<<"the solutionis="<<a+(b-c/d) + ++num;
   return 0;
}
